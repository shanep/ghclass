{
  "name": "@shanepanter/ghclass",
  "description": "Clone student repos created from classroom.github.com",
  "version": "1.0.0",
  "author": "Shane K. Panter",
  "license": "MIT",
  "homepage": "https://github.com/shanep/ghclass#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanep/ghclass.git"
  },
  "bugs": {
    "url": "https://github.com/shanep/ghclass/issues"
  },
  "keywords": [
    "education",
    "classroom.github.com"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "start": "tsx ./src/index.ts",
    "build": "tsc -p .",
    "test": "node --loader tsx --test test/*.test.ts",
    "bundle": "npm run format:write && npm run package",
    "format:write": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "package": "ncc build src/index.ts --license licenses.txt",
    "package:watch": "npm run package -- --watch",
    "all": "npm run format:write && npm run lint && npm run test && npm run package"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "csv-parse": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.3",
    "@vercel/ncc": "^0.38.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  }
}
